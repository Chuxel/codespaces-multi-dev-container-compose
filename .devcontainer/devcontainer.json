{
	"name": "Boostrap Container",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "workspace",
	"workspaceFolder": "/workspace",

	"forwardPorts": [ 9256 ],
	"portsAttributes": {
		"9256": {
			"label": "Docker TCP Port",
			"onAutoForward": "ignore",
			"requireLocalPort": true
		}
	},
	"postStartCommand": "bash .devcontainer/forward-docker-socket",
	"remoteUser": "vscode"
}
